           {% if section.settings.overlay_4 %} <div style="background:{{ section.settings.overlay_bgcolor_4 | color_modify: 'alpha', 0.5 }};" class="ovrly"></div>{% endif %}
            {% else %}
             <img src="//via.placeholder.com/1200x493">
            {% endif%}           
            {% if section.settings.link_4 != blank %}  </a>
            {% endif%}
            <div class="featured-content">              
            {% if section.settings.special_title_3 != blank %}
            <h6 style="color:{{ section.settings.special_title_3_clr }};">{{ section.settings.special_title_3 }}</h6>
            {% endif %}
            {% if section.settings.title_4 != blank %}
