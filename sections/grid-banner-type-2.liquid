              {% endif %}
            </div>
          </div>
        </div> 
        <div class="grid__item wide--one-third post-large--one-third large--one-third medium-down--one-third small--grid__item">        
          <div class="ovrly18">
            {% if section.settings.link_5 != blank %}<a href="{{ section.settings.link_5 }}">{% endif%}
            {% if section.settings.image_5 != blank %}
            <img src="{{ section.settings.image_5 | img_url: '1920X' }}" alt="{{ section.settings.title_5 }}" />
            {% if section.settings.overlay_5 %} <div style="background:{{ section.settings.overlay_bgcolor_5 | color_modify: 'alpha', 0.5 }};"  class="ovrly"></div> {% endif %}
            {% else %}
            <img src="//via.placeholder.com/568x460">
            {% endif%}           
            {% if section.settings.link_5 != blank %}  </a>
            {% endif%}
            <div class="featured-content last_banner_img">   
             {% if section.settings.special_title_4 != blank %} 
 			<h6 style="color:{{ section.settings.special_title_4_clr }};">{{ section.settings.special_title_4 }}</h6>
            {% endif %}
            {% if section.settings.title_5 != blank %}
            <h2 style="color:{{ section.settings.title_5_clr }};">{{ section.settings.title_5}}</h2>    
            {% endif %} 
              {% if section.settings.text_5 != blank %}
              <h5 style="color:{{ section.settings.text_5_clr }};">{{ section.settings.text_5}}</h6>    
              {% endif %}  
              {% if section.settings.button_5 != blank %}
              <a class="btn five" href="{{ section.settings.link_5 }}" style="color:{{ section.settings.button_5_txt_clr }};border:none;background-color:{{ section.settings.button_5_bg_clr }};">{{ section.settings.button_5}}</a>    
              {% endif %}         
            </div>   
          </div>
     
      </div>
      <style>
        .grid__item.full-width-grid-banner .ovrly18:after{ background:{{ section.settings.shape_clr }};}
       @media screen and (min-width: 320px) and (max-width: 767px) { .grid-banner-type-2 .full-width-grid-banner .featured-content{ background:{{ section.settings.content_clr | color_modify: 'alpha', 0.8 }};}}
        .grid-banner-type-2 .featured-content .btn.one:hover { background:{{ section.settings.button_1_bg_hover_clr }} !important;color:{{ section.settings.button_1_text_hover_clr }} !important; }
        .grid-banner-type-2 .featured-content .btn.two:hover { background:{{ section.settings.button_2_bg_hover_clr }} !important; color:{{ section.settings.button_2_text_hover_clr }} !important;}
        .grid-banner-type-2 .featured-content .btn.three:hover { background:{{ section.settings.button_3_bg_hover_clr }} !important;color:{{ section.settings.button_3_text_hover_clr }} !important; }
        .grid-banner-type-2 .featured-content .btn.four:hover { background:{{ section.settings.button_4_bg_hover_clr }} !important;color:{{ section.settings.button_4_text_hover_clr }} !important; }
        .grid-banner-type-2 .featured-content .btn.five:hover { background:{{ section.settings.button_5_bg_hover_clr }} !important;color:{{ section.settings.button_5_text_hover_clr }} !important; }
      </style>

    </div>
  </div>    
  </div>
</div>
</div>


