<div data-section-id="{{ section.id }}" data-section-type="grid-banner-type-2" class="grid-banner-type-2" style="background:{{ section.settings.bg_color}}";>   
  <div class="grid-uniform featuredItems"> 
    <div class="container-bg">
    <div class="grid__item wide--one-quarter post-large--one-quarter large--one-quarter medium-down--grid__item small--grid__item full-width-grid-banner">    
      <div class="grid__item full-width-grid-banner">

        <div class="ovrly18">
          {% if section.settings.link_1 != blank %}<a href="{{ section.settings.link_1 }}">{% endif%}
          {% if section.settings.image_1 != blank %}

          <img src="{{ section.settings.image_1 | img_url: '1920X' }}" alt="{{ section.settings.title_1 }}" />
         {% if section.settings.overlay_1 %} <div class="ovrly" style="background:{{ section.settings.overlay_bgcolor_1 | color_modify: 'alpha', 0.5 }};"></div> {% endif %}
          {% else %}
          <img src="//via.placeholder.com/568x926">
          
          {% endif%}           
          {% if section.settings.link_1 != blank %}  </a>
          {% endif%}
          <div class="featured-content">       
            {% if section.settings.special_title != blank %}
            <h6 style="color:{{ section.settings.special_title_clr }};">{{ section.settings.special_title }}</h6>
            {% endif %}
            {% if section.settings.title_1 != blank %}
            <h2 style="color:{{ section.settings.title_1_clr }};">{{ section.settings.title_1}}</h2>    
            {% endif %}  
            {% if section.settings.button_1 != blank %}
            <a class="btn one" href="{{ section.settings.link_1 }}" style="color:{{ section.settings.button_1_txt_clr }};border:none;background-color:{{ section.settings.button_1_bg_clr }};">{{ section.settings.button_1}}</a>    
            {% endif %}         
          </div>   
        </div>

      </div>
    </div>
    <div class="grid__item wide--three-quarters post-large--three-quarters large--three-quarters medium-down--grid__item small--grid__item grid_banner_right">      
      <div class="grid__item grid_banner_bottom">   
        <div class="grid__item wide--one-third post-large--one-third large--one-third grid_banner_left">    
          <div class="ovrly18">
            {% if section.settings.link_2 != blank %}<a href="{{ section.settings.link_2 }}">{% endif%}
            
            
            {% if section.settings.image_2 != blank %}
            <img src="{{ section.settings.image_2 | img_url: '1920X' }}" alt="{{ section.settings.title_2 }}" />
            {% if section.settings.overlay_2 %}<div class="ovrly" style="background:{{ section.settings.overlay_bgcolor_2 | color_modify: 'alpha', 0.5 }};"></div>{% endif%} 
            {% else %}
            <img src="//via.placeholder.com/568x460">
            {% endif%}           
            {% if section.settings.link_2 != blank %}  </a>
            {% endif%}
            <div class="featured-content first">       
             {% if section.settings.special_title_1 != blank %}
            <h6 style="color:{{ section.settings.special_title_1_clr }};">{{ section.settings.special_title_1 }}</h6>
            {% endif %}
            {% if section.settings.title_2 != blank %}
            <h2 style="color:{{ section.settings.title_2_clr }};">{{ section.settings.title_2}}</h2>    
            {% endif %}  
              {% if section.settings.button_2 != blank %}
              <a class="btn two" href="{{ section.settings.link_2 }}" style="color:{{ section.settings.button_2_txt_clr }};border:none;background-color:{{ section.settings.button_2_bg_clr }};">{{ section.settings.button_2}}</a>    
              {% endif %}         
            </div> 
          </div>
        </div>
        <div class="grid__item wide--two-thirds post-large--two-thirds large--two-thirds">
          <div class="ovrly18">
            {% if section.settings.link_3 != blank %}<a href="{{ section.settings.link_3 }}">{% endif%}
            {% if section.settings.image_3 != blank %}
            <img src="{{ section.settings.image_3 | img_url: '1920X' }}" alt="{{ section.settings.title_3 }}" />
            {% if section.settings.overlay_3 %}<div style="background:{{ section.settings.overlay_bgcolor_3 | color_modify: 'alpha', 0.5 }};" class="ovrly"></div>{% endif %}
            {% else %}
              <img src="//via.placeholder.com/1200x493">
            {% endif%}           
            {% if section.settings.link_3 != blank %}  </a>
            {% endif%}
            <div class="featured-content">       
               {% if section.settings.special_title_2 != blank %}
            <h6 style="color:{{ section.settings.special_title_2_clr }};">{{ section.settings.special_title_2 }}</h6>
            {% endif %}
            {% if section.settings.title_3 != blank %}
            <h2 style="color:{{ section.settings.title_3_clr }};">{{ section.settings.title_3}}</h2>    
            {% endif %}  
              {% if section.settings.text_3 != blank %}
              <h5 style="color:{{ section.settings.text_3_clr }};">{{ section.settings.text_3}}</h6>    
              {% endif %}  
              {% if section.settings.button_3 != blank %}
              <a class="btn three" href="{{ section.settings.link_3 }}" style="color:{{ section.settings.button_3_txt_clr }};border:none;background-color:{{ section.settings.button_3_bg_clr }};">{{ section.settings.button_3}}</a>    
              {% endif %}               
            </div>   
          </div>
        </div>
      </div>
      <div class="grid__item grid_banner_2_bottom">   

        <div class="grid__item wide--two-thirds post-large--two-thirds large--two-thirds medium-down--two-thirds small--grid__item">        
